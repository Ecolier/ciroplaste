    "@payloadcms/storage-s3": "^3.0.1",
    "cross-env": "^7.0.3",
    "graphql": "^16.8.1",
    "next": "15.0.3",
    "payload": "^3.0.1",
    "react": "19.0.0-rc-66855b96-20241106",
