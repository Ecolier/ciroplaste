datasource sqlite {
  url               = env("DATABASE_URL")
  shadowDatabaseUrl = env("SHADOW_DATABASE_URL")
  provider          = "sqlite"
}

generator client {
  provider = "prisma-client-js"
}

model Article {
  id       String @id @default(cuid())
  title    String @default("")
  subtitle String @default("")
  body     String @default("")
}
