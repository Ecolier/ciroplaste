{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "build", "dist"],
      "env": [
        "CMS_SCRET",
        "NEXT_PUBLIC_CONTENT_BASE_URL",
        "NEXT_PUBLIC_ASSETS_BASE_URL",
        "NEXT_PUBLIC_INSTAGRAM_LINK",
        "NEXT_PUBLIC_GITHUB_LINK"
      ]
    }
  }
}
